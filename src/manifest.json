{
  "name": "Perkeep Browser Extension",
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "history",
    "storage"
  ]
}
